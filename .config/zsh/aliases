bindkey '^f' fzf-cd-widget
bindkey -s "^g" 'git-main^M'
bindkey '^ ' autosuggest-accept

alias ls="exa --icons --grid --classify --colour=auto --sort=type --group-directories-first --header --modified --created --git --binary"
alias la="ls -a"
alias ll="ls -al"
alias copy='xclip -r -selection clipboard'
alias pir='gh pr list | cut -f1,2 | gum choose | cut -f1 | xargs gh pr checkout'
alias e='lvim ~/random'
alias fonts='fc-list : family | sort | uniq'
alias code='code-oss'
